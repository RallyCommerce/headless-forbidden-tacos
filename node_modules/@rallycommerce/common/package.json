{
  "name": "@rallycommerce/common",
  "version": "0.12.1",
  "description": "A library for common styles, utils and shared web components",
  "main": "index.js",
  "type": "module",
  "exports": {
    "./*": "./dist/*",
    "./styles/": "./styles/"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "prepack": "rm -rf dist/ && npm run build",
    "prepublishOnly": "npm pack",
    "start": "web-dev-server --open dev/ --node-resolve",
    "start:watch": "web-dev-server --open dev/ --node-resolve --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RallyCommerce/fe-common.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RallyCommerce/fe-common/issues"
  },
  "homepage": "https://github.com/RallyCommerce/fe-common#readme",
  "peerDependencies": {
    "deepmerge": "^4.2.2",
    "lit": "^2.2.6",
    "rxjs": "~7.5.5"
  },
  "devDependencies": {
    "typescript": "~4.4.4",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "@web/dev-server": "^0.1.22",
    "deepmerge": "^4.2.2",
    "lit": "^2.2.6",
    "rxjs": "~7.5.5"
  }
}