{
  "name": "@rallycommerce/checkout-button",
  "version": "1.6.0",
  "description": "The Rally Checkout Button is a replacement for your default checkout buttons. It takes you to Rally's checkout page(s) instead of the platforms default checkout page.",
  "main": "./build/index.cjs.js",
  "license": "ISC",
  "module": "./build/index.esm.js",
  "unpkg": "./build/index.umd.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.esm.js",
      "require": "./build/index.cjs.js"
    }
  },
  "type": "module",
  "scripts": {
    "build": "configuration=local rollup -c",
    "local": "configuration=local rollup -c",
    "production": "configuration=production rollup -c",
    "package": "configuration=production rollup -c rollup.npm-config.js",
    "develop": "configuration=develop rollup -c && node generate-package.js",
    "sandbox": "configuration=sandbox rollup -c && node generate-package.js",
    "stage": "configuration=stage rollup -c && node generate-package.js",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "serve": "wds --watch",
    "serve:prod": "MODE=prod npm run serve",
    "checksize": "rollup -c ; cat checkout-button.bundled.js | gzip -9 | wc -c ; rm checkout-button.bundled.js",
    "prepack": "npm run package",
    "prepublishOnly": "npm pack",
    "test": "jest",
    "test:local": "jest --watch"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "author": "Rally Commerce Inc.",
  "dependencies": {
    "@rallycommerce/common": "^0.12.1",
    "deepmerge": "^4.2.2",
    "lit": "^2.1.0",
    "rxjs": "~7.5.5"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.5.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/jest": "^28.1.5",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@web/dev-server": "^0.1.22",
    "@web/dev-server-legacy": "^0.1.4",
    "@web/dev-server-rollup": "^0.3.9",
    "@web/rollup-plugin-copy": "^0.3.0",
    "@web/rollup-plugin-html": "^1.10.2",
    "@web/rollup-plugin-polyfills-loader": "^1.2.0",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "css-loader": "^5.1.1",
    "deepmerge": "^4.2.2",
    "eslint": "^7.32.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lit-analyzer": "^0.0.6",
    "mini-css-extract-plugin": "^1.3.9",
    "mocha": "^9.1.1",
    "prettier": "^2.3.2",
    "rollup": "^2.70.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-summary": "^1.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "ts-jest": "^28.0.6",
    "typescript": "4.4.3",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0"
  },
  "customElements": "custom-elements.json",
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "preset": "ts-jest/presets/default-esm",
    "transform": {
      ".(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@testing/(.*)$": "<rootDir>/testing/$1",
      "environment": "<rootDir>/testing/environment",
      "^rxjs(.*)$": "<rootDir>/node_modules/rxjs/dist/cjs$1/index.js",
      "^@rallycommerce/common/utils$": "<rootDir>/testing/stubs/utils.stub.ts"
    },
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/elements/",
      "/build/"
    ]
  }
}
